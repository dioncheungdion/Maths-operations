public class Main {
    public static void main(String[] args) {
        // Variables
        int a = 10;
        int b = 3;

        //Division
        int div = a/b;               // answer will round to whole number because variable is set to integer
        System.out.println(a+ " / " + b + " = " + div);

        //Modulus
        int mod = a%b;
        System.out.println(a + " % " + b + " = " + mod);

        //Rounding
        double num = 5.67;
        long rounded = Math.round(num);   //long is for whole numbers, takes more numbers than int
        System.out.println("Round(" + num + ") = " + rounded); //5.67 is rounded to the nearest whole number

        //Floor and ceiling
        double floor = Math.floor(num); // returns 5.0
        double ceil = Math.ceil(num); // returns 6.0
        System.out.println("Floor("+ num + ") = " + floor);
        System.out.println("Ceil("+ num + ") = " + ceil);
    }
}
